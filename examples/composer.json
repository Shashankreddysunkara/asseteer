{

  "require": {
    "figdice/asseteer": "dev-master",

    "phpmailer/phpmailer": "~5.2",
    "monolog/monolog": "~1.13",


    "static-assets/jquery": "2.1.3",
    "static-assets/foundation-css": "5.5.0",
    "static-assets/foundation-js": "5.5.0",
    "static-assets/foundation.dropdown": "5.5.0",
    "static-assets/foundation-icons": "3.0.0",
    "static-assets/foundation-icons-woff": "3.0.0",
    "static-assets/foundation-icons-ttf": "3.0.0"
  },

  "repositories":[
    {
      "type": "package",
      "package": {
        "name": "static-assets/jquery",
        "version": "2.1.3",
        "dist": {
          "url": "http://code.jquery.com/jquery-2.1.3.min.js",
          "type": "file"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "static-assets/foundation-css",
        "version": "5.5.0",
        "dist": {
          "url": "http://cdnjs.cloudflare.com/ajax/libs/foundation/5.5.0/css/foundation.min.css",
          "type": "file"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "static-assets/foundation-js",
        "version": "5.5.0",
        "dist": {
          "url": "http://cdnjs.cloudflare.com/ajax/libs/foundation/5.5.0/js/foundation.min.js",
          "type": "file"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "static-assets/foundation.dropdown",
        "version": "5.5.0",
        "dist": {
          "url": "http://cdnjs.cloudflare.com/ajax/libs/foundation/5.5.0/js/foundation/foundation.dropdown.min.js",
          "type": "file"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "static-assets/foundation-icons",
        "version": "3.0.0",
        "dist": {
          "url": "http://cdnjs.cloudflare.com/ajax/libs/foundicons/3.0.0/foundation-icons.css",
          "type": "file"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "static-assets/foundation-icons-woff",
        "version": "3.0.0",
        "dist": {
          "url": "http://cdnjs.cloudflare.com/ajax/libs/foundicons/3.0.0/foundation-icons.woff",
          "type": "file"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "static-assets/foundation-icons-ttf",
        "version": "3.0.0",
        "dist": {
          "url": "http://cdnjs.cloudflare.com/ajax/libs/foundicons/3.0.0/foundation-icons.ttf",
          "type": "file"
        }
      }
    }
  ],

  "extra": {
    "post-install-asseteer": [{
      "vendor": "static-assets",
      "target": "app/http/js",
      "filters": [ "\\.js$" ]
    },{
      "vendor": "static-assets",
      "target": "app/http/css",
      "filters": [ "\\.css$" ]
    },{
      "vendor": "static-assets/foundation-icons-woff",
      "target": "app/http/css/foundation-icons",
      "filters": [ "\\.woff$" ]
    },{
      "vendor": "static-assets/foundation-icons-ttf",
      "target": "app/http/css/foundation-icons",
      "filters": [ "\\.ttf$" ]
    }]
  },

  "scripts" :{
    "post-install-cmd": [
      "asseteer\\AssetInstaller::postInstall"
    ]
  }
}
